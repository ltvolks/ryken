#footer {
	width:960px;
	margin:0 auto;
}
#digits {
	float:left;
	margin-left:70px;
	text-align:left;
}
#digits p {
	margin:2px 0;
	font-size:1.2em;
	color:#6497bb;
	clear:both;
}
#digits p span.label {
	display:block;
	float:left;
	width:3.7em;
}
#digits a:link, #digits a:visited {
	color:#6497bb;
}
#digits a:hover, #digits a:active, #digits a:focus {
	color:#fff;
	text-decoration:underline;
}
#digits #phone {
	color:#b9cf42;
	font-size:1.9em;
	margin:15px 0 5px 0;
	font-weight:bold;
}
#map {
	float:left;
	margin-left:50px;
	text-align:left;
	font-size:1.2em;
	line-height:1.2em;
}
#map a:link, #map a:visited {
	display:block;
	padding:15px 15px 30px 90px;
	background:url(../images/map_icon.png) 10px 10px no-repeat;
	color:#6497bb;
}
#map a:hover, #map a:active, #map a:focus {
	background:url(../images/map_icon.png) 10px 10px no-repeat #346081;
	color:#6497bb;
	text-decoration:none;
	
}
#map a:link span, #map a:visited span {
	color:#fff;
}
#map a:hover span, #map a:active span, #map a:focus span {
	color:#fff;
	text-decoration:underline
}
#copyright {
	color:#2f556f;
	float:right;
	text-align:right;
	margin-right:40px;
	font-size:1.2em;
	line-height:1.4em;
	background:url(../images/copyright_bg.png) top left no-repeat;
	padding:10px 0 0 75px;
	width:auto;
}
#copyright p {
	margin:0;
	padding-bottom:10px;
